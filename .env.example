# Example environment variables for CKS Lab configuration
# Copy this file to .env and update the values

# SSH Public Key for VM access
SSH_PUBLIC_KEY="ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAABgQC7..."

# Encryption secret for Kubernetes
# Generate with: openssl rand -base64 32
ENCRYPTION_SECRET="YourEncryptionSecretHere"

# AWS Configuration (if using AWS)
AWS_REGION="us-east-1"
AWS_KEY_PAIR="your-key-pair-name"
AWS_SECURITY_GROUP="sg-xxxxxxxxxx"
AWS_SUBNET="subnet-xxxxxxxxxx"
AWS_INSTANCE_TYPE="t3.medium"

# Advanced Configuration
KUBERNETES_VERSION="v1.32.2"
CALICO_VERSION="v3.27.3"
FALCO_VERSION="0.39.1"